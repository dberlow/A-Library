# ==================================================# OpenType Features for GrandCentral -Light# Generated on 2013-11-04 16:58:47# Updated 131105 KL, includes contextual arrows# ==================================================	@ordn_digits = [zero one two three four five six seven eight nine];	@figures = [zero one two three four five six seven eight nine];	@big_off = [registered];	@big = [registered.big];	@ornm = [apple uni2022.ornm_arrowleft_head uni2022.ornm_arrowleft_tail uni2022.ornm_arrow_shaft uni2022.ornm_arrowright_tail uni2022.ornm_arrowright_head];	languagesystem DFLT dflt;	languagesystem latn dflt;	feature ordn { # Ordinals		sub @ordn_digits a' by ordfeminine;		sub @ordn_digits o' by ordmasculine;	} ordn;	feature liga { # Standard Ligatures		sub R A by R_A;	} liga;	feature ss01 { # Contextual Arrows		sub [less greater]' [hyphen uni2022.ornm_arrow_shaft] by [uni2022.ornm_arrowleft_head uni2022.ornm_arrowright_tail];		sub [uni2022.ornm_arrowleft_head uni2022.ornm_arrowright_tail uni2022.ornm_arrow_shaft hyphen] hyphen' by uni2022.ornm_arrow_shaft;		sub hyphen' [less greater hyphen] by uni2022.ornm_arrow_shaft;		sub [hyphen uni2022.ornm_arrow_shaft] [less greater]' by [uni2022.ornm_arrowleft_tail uni2022.ornm_arrowright_head];	} ss01;			feature salt { # Stylistic Alternates		sub @big_off by @big;	} salt;	feature ornm { # Ornaments		sub bullet from @ornm;	} ornm;